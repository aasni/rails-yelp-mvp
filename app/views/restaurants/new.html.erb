<h1>Restaurants#new</h1>
<p>Find me in app/views/restaurants/new.html.erb</p>
<h1>HELLO GIGABIT Restaurant</h1>

<%= simple_form_for @restaurant do |t| %>
  <%= t.input :name %>
  <%= t.input :address %>
  <%= t.input :phone_number %>
  <%= t.input :category, collection: ["chinese", "italian", "japanese", "french", "belgian"] %>
  <%= t.submit "Create new restaurant", class: "btn btn-primary" %>
<% end %>
